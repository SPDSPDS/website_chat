{
    "manifest_version": 2,
    "name": "WebChat: Chat With WebPages",
    "version": "1.0",
    "permissions": ["http://localhost:8000/",
        "tabs",
        "fileSystem",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "icon2.png"
    },
    "icons": {
        "48": "icon.png"
    }
}
